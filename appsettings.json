{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "telerikReporting": {
    "dpiAware": {
      "dpiAwareness": "PROCESS_SYSTEM_DPI_AWARE"
    },
    "extensions": [
      {
        "name": "IMAGEPrint",
        "parameters": [
          {
            "name": "OutputFormat",
            "value": "PNG"
          }
        ]
      }
    ],
    "fontLibrary": {
      "useDefaultLocations": "false",
      "fontLocations": [
        {
          "path": "/usr/share/fonts",
          "searchSubfolders": "true"
        }
      ]
    }
  }
}
